    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tsc-alias": "^1.8.10",
    "tsconfig": "*",
    "turbo": "latest"
  },
  "name": "finpro-nextjs-express-prisma",
  "packageManager": "npm@8.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
