{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", ".vercel/**"],
      "env": [
        "NEXT_PUBLIC_API_BASE_URL",
        "NEXT_PUBLIC_OPEN_ROUTER_API_KEY",
        "NEXT_PUBLIC_OPEN_ROUTER_API_KEY_2",
        "NEXT_PUBLIC_OPEN_ROUTER_API_KEY_3",
        "NEXT_PUBLIC_OPEN_ROUTER_API_KEY_4",
        "NEXT_PUBLIC_OPEN_ROUTER_API_KEY_5",
        "AUTH_SECRET",
        "NEXTAUTH_URL"
      ]
    }
  }
}
